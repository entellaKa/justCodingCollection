#temp = "3+4*(1+4/2+2*3)+(25/37)"
temp = "123456+345678*567890"
 
def cal(inputValue):
	formula = "(" + inputValue[0]
	stack = 1 #Bracket Count
 
	for i in range(1, len(inputValue)):
		
		if inputValue[i].isnumeric():
			
		elif inputValue[i] != ")":
			formula += inputValue[i]
	return formula
p = cal(temp)
print("식 정리 : "+p)
